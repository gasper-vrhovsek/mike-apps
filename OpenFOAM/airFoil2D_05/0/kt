FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      kt;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1.5e-08;

boundaryField
{
    body
    {
        type            fixedValue;
        value           uniform 0;
    }
    exterior
    {
        type            freestream;
        freestreamValue uniform 1.5e-08;
    }
    sides
    {
        type            empty;
    }
}


